<?xml version="1.0"?>

<configuration>

	<system.web>
		<compilation debug="true" targetFramework="4.0" />
	</system.web>

	<system.webServer>
    <!-- 
      Configuring for Gate.Hosts.AspNet.Module for AppDomain.
      This bypasses the normal native/managed module pipeline.
      -->

    <!--<modules runAllManagedModulesForAllRequests="true">
			<add name="Gate" type="Gate.Hosts.AspNet.Module, Gate.AspNet" />
		</modules>-->

    <!-- 
      Configuring for Gate.Hosts.AspNet.Handler for all requests.
      This also allows existing modules to run. 
      -->
    <handlers>
      <add name="Gate" path="*" verb="*" type="Gate.Hosts.AspNet.Handler, Gate.Hosts.AspNet" />
    </handlers>
	</system.webServer>

</configuration>